{
  "name": "potato-server",
  "version": "0.0.1",
  "description": "Potato API Server",
  "main": "www.ts",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf ./dist",
    "prestart:prod": "yarn run build:clean && yarn run build",
    "start:prod": "cross-env NODE_ENV=prod ts-node -r tsconfig-paths/register dist/www.js",
    "start:local": "cross-env NODE_ENV=local nodemon --exec ts-node -r tsconfig-paths/register src/www.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit --runInBand",
    "test:watch": "cross-env NODE_ENV=test jest --watch --detectOpenHandles --forceExit --runInBand"
  },
  "keywords": [],
  "author": "seungh0",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "routing-controllers-openapi": "^2.2.0",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.1.6",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "aws-sdk": "^2.841.0",
    "axios": "^0.21.1",
    "better-sqlite3": "^7.1.2",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.2",
    "class-validator": "^0.13.1",
    "class-validator-jsonschema": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.2.5",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0-alpha.6",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.30",
    "typeorm-typedi-extensions": "^0.4.0"
  }
}
